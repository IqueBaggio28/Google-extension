{
  "name": "Pomodoro Timer",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_icon": "Pomodoro.png"
  },
  "permissions": ["storage","notifications","activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["style.css"]
    }
  ]
}